                                                    <a href="{{ path('blog') }}"
                                                       class="level-top"><span>{{ 'header.menu-blog'|trans }}</span></a>
                                                </li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                                <!-- navigation EOF -->                            </div>
                        </div>
                        <div class="header-logo">
                            <a href="{{ path('homepage') }}" title="{{ siteName }}" class="logo">
                                <img class="x1" src="{{ optipng('bundles/app/images/logo_unik.png', { 'output' : 'images/logo_unik.png'}) }}" alt="{{ siteName }}"/>
                            </a>
                        </div>
                        <div class="header-top-right">
                            <div class="top-right-inner">
                                <div class="header-type">
                                    <div class="header-maincart">
                                        <div class="cart-container">
                                            <a href="{{ path('cart') }}" title="Mon Panier" class="icon-cart-header skip-link">
                                                <span class="total"><strong class="fa fa-shopping-bag" aria-hidden="true"></strong><span class="hidden-mobile">{{ 'header.my-cart'|trans }}</span> <span class="numb">{{ app.session.get('cart')|length }}</span></span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="header-setting">
                                    <div class="setting-switcher switcher-wrap">
