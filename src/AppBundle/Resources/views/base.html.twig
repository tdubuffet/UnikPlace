<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>{% block title %}{{ (app.request.get('_route') ~ '.title')|trans({}, 'seo')  }} {% endblock %} {{ ('default.title'|trans({"%siteName%" :  siteName}, 'seo')) }}</title>

    {% set metaDescription =  (app.request.get('_route') ~ '.description')|trans({}, 'seo')  %}

    {% if metaDescription == '' or  metaDescription == (app.request.get('_route') ~ '.description') %}
      {% set metaDescription = 'default.description'|trans({"%siteName%" :  siteName}, 'seo') %}
    {% endif %}

    <meta name="description" content="{% block description %}{{ metaDescription }}{% endblock %}">

    {% block meta %}{% endblock %}

    <!-- Bootstrap -->
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/master.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
