  </head>
  <body {% block bodyclass %}{% endblock %}>


    <!-- // HEADER // -->
    <header class="header-wrapper">
      <div class="header-container">
        <div class="main-header">
          <div class="container">
            <div class="header-content mobile-wrapper visible-xs">
              <div id="mobile-sticky" class="mobile-content-inner">
                <div class="header-top-left">
                  <div class="top-left-inner">
                    <div class="header-search">
                      <div class="search-container">
                        <div class="overwrite-setting skip-link" onclick="Common.mobileSkipLink(this);" data-target-element="#navbar-search">
                          <i class="fa fa-search"></i>
                        </div>
                        <div id="navbar-search" class="switcher-content skip-content">
                          <form id="search_mb_mini_form" action="{{ path('search') }}" method="get">
                            <div class="input-group form-search">
                              <span class="input-group-btn category-filter">
                                <button type="button" class="btn dropdown-toggle" data-toggle="dropdown">
                                  <span class="category-label">
                                    Tout </span>&nbsp;<i class="fa fa-angle-down"></i>
                                </button>
                                {{ render(controller('AppBundle:Default:searchcategories')) }}
                              </span>
                              <input type="hidden" name="cat" class="search-category" value="" />
                              <input id="mb_search"
                                     type="search"
                                     name="q"
                                     value="{{ app.request.query.get('q') }}"
                                     class="input-text required-entry"
                                     maxlength="128"
                                     placeholder="Rechercher sur le site..."
                                     />
                              <button type="submit" title="Search" class="search-button"><span><span><i class="fa fa-search"></i></span></span></button>
                            </div>
                            <div id="search_mb_autocomplete" class="search-autocomplete" style="display: none"></div>
                          </form>
                        </div>
                      </div>
                    </div>
                    <!-- navigation BOF -->
                    <div class="navbar visible-xs">
                      <div class="navbar">
                        <div class="navbar-header skip-link" onclick="Common.mobileSkipLink(this);" data-target-element="#navbar-toggle">
                          <button class="navbar-toggle collapsed" type="button" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                          </button>
                        </div>
                        <nav id="navbar-toggle" class="bs-navbar-collapse skip-content" role="navigation">
                          <ul class="nav-accordion nav-mobile-accordion">

                            <li class="level0 home level-top">
                              <a href="{{ path('homepage') }}"
                                 class="level-top"><span>Accueil</span></a>
                            </li>
                            {{ render(controller('AppBundle:Default:mobilecategories')) }}
                          </ul>
                        </nav>
                      </div>
                    </div>
                    <!-- navigation EOF -->                            </div>
                </div>
                <div class="header-logo">
                  <a href="{{ path('homepage') }}" title="Magento Commerce" class="logo">
                    <img class="x1" src="http://elara.icotheme.com/media/wysiwyg/icotheme/elara/logo/logo_4.png" alt="Magento Commerce" />
                  </a>
                </div>
                <div class="header-top-right">
                  <div class="top-right-inner">
                    <div class="header-type">
                      <div class="header-maincart">
                        <div class="cart-container">
                          <a href="{{ path('cart') }}" title="Mon Panier" class="icon-cart-header skip-link">
                            <span class="total"><strong class="fa fa-shopping-bag" aria-hidden="true"></strong><span class="hidden-mobile">Mon Panier</span> <span class="numb">{{ app.session.get('cart')|length }}</span></span>
                          </a>
                        </div>
                      </div>
                    </div>
                    <div class="header-setting">
                      <div class="setting-switcher switcher-wrap">
                        <div class="overwrite-setting skip-link" onclick="Common.mobileSkipLink(this);" data-target-element="#navbar-setting">
                          <span class="fa fa-cog"></span>
                        </div>
                        <div id="navbar-setting" class="switcher-content skip-content">
                          <div class="main-switcher">
                            <div class="main-setting">
                              <div class="main-account">
                                <ul class="links">
                                  {% if app.user %}
                                    <li class="first" ><a class="my-account" href="{{ path('fos_user_profile_edit') }}" title="Mes Paramètres">Mes Paramètres</a></li>
                                    <li><a href="{{ path('user_account_wishlist') }}" title="Ma Wishlist" class="link-wishlist">Ma Wishlist</a></li>
                                    <li><a class="checkout" href="{{ path('cart') }}" title="Mon Panier" class="top-link-checkout">Mon Panier</a></li>
                                    <li class=" last" ><a class="log-in" href="{{ path('fos_user_security_logout') }}" title="Déconnexion">Déconnexion</a></li>
                                  {% else %}
                                    <li class="first" ><a href="{{ path('fos_user_security_login') }}" title="Connexion" >Connexion</a></li>
                                    <li class="last" ><a href="{{ path('fos_user_registration_register') }}" title="Connexion" >Inscription</a></li>
                                  {% endif %}
                                </ul>
                              </div>
                            </div>
                            <div class="language-currency ">
                              <div class="language-switcher">
                                <div class="title-selector"><span>Langues</span></div>
                                <ul class="select-language clearfix">
                                  <li class="current-language">
                                    <span>
                                      <img alt="francais" src="http://elara.icotheme.com/media/wysiwyg/icotheme/elara/flags/flag-french.jpg" />
                                    </span>
                                  </li>
                                  <li><a href="#"> <img alt="anglais" src="http://elara.icotheme.com/media/wysiwyg/icotheme/elara/flags/flag-english.jpg" /></a></li>
                                </ul>
                              </div>
                            </div>

                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
