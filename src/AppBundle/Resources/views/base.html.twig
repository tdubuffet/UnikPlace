                                        <div class="language-currency">
                                            <div class="language-switcher switcher-wrap">

                                                <div class="overwrite-language">
                                                    <span style="background-image:url('{{ asset('bundles/app/images/flags/flag-french.jpg') }}');background-repeat: no-repeat; background-position: 15px center;">Français</span>
                                                </div>
                                                <ul class="select-language">
                                                    <li><a style="background-image:url({{ asset('bundles/app/images/flags/flag-french.jpg') }}); background-repeat: no-repeat; background-position: 0px center;" href="{{ path('homepage', {'_locale': 'fr'}) }}"> Français</a></li>
                                                    <li><a style="background-image:url({{ asset('bundles/app/images/flags/flag-english.jpg') }}); background-repeat: no-repeat; background-position: 0px center;" href="{{ path('homepage', {'_locale': 'en'}) }}"> English</a></li>
                                                </ul>

                                            </div>
                                        </div>
                                    </div>
                                    <div class="store-wrapper"></div>
                                    <div class="header-setting">
                                        <div class="dropdown setting-switcher switcher-wrap">
                                            {% if app.user %}
                                                <a href="#" class="overwrite-setting dropdown-toggle" data-toggle="dropdown">
                                                    <span>{{ 'header.account-menu-title'|trans }}</span>
                                                </a>
                                                <ul class="dropdown-menu dropdown-user">
                                                    <li class="first"><a href="{{ path("user_account_products") }}">{{ 'header.account-menu-products'|trans }}</a></li>
                                                    <li><a href="{{ path('user_account_wallet') }}">{{ 'header.account-menu-wallet'|trans }}</a></li>
                                                    <li><a href="{{ path('fos_user_profile_edit') }}">{{ 'header.account-menu-profil'|trans }}</a></li>
                                                    <li><a href="{{ path('user_account_purchases') }}">{{ 'header.account-menu-purchases'|trans }}</a></li>
                                                    <li><a href="{{ path('user_account_sales') }}">{{ 'header.account-menu-sales'|trans }}</a></li>
