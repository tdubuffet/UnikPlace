                                                    <span>{{ 'header.account-menu-title'|trans }}</span>
                                                </a>
                                            {% endif %}
                                        </div>
                                    </div>
                                </div>
                                <div class="header-bottom-container">
                                    <div class="row">
                                        <div class="header-search">
                                            <div class="search-container">
                                                <div class="main-form-search">
                                                    <form id="search_mini_form" action="{{ path('search') }}" method="get">
                                                        <div class="input-group form-search">
                                                            <div class="input-group-btn category-filter">
                                                                <button type="button" class="btn dropdown-toggle" data-toggle="dropdown">
                                                                    <span class="category-label">Tout </span>&nbsp;
                                                                    <i class="fa fa-angle-down"></i>
                                                                </button>
                                                                {{ render(controller('AppBundle:Default:searchcategories')) }}
                                                            </div>
                                                            <input type="hidden" name="cat" class="search-category" value="{{ app.request.query.get('cat') }}"/>
                                                            <input id="search"
                                                                   type="search"
                                                                   name="q"
                                                                   value="{{ app.request.query.get('q') }}"
                                                                   class="input-text required-entry"
                                                                   maxlength="128"
                                                                   placeholder="{{ 'header.search-placeholder'|trans }}"/>
                                                            <button type="submit" title="Search" class="button">
                                                                <span><span><i class="fa fa-search"></i></span></span>
                                                            </button>
                                                        </div>
                                                        <div id="search_autocomplete" class="search-autocomplete" style="display: none"></div>
                                                    </form>


                                                </div>
                                            </div>
                                        </div>

                                        <div class="header-account-action hidden-md hidden-xs hidden-sm" id="notificationLi">
                                            <div class="account-container">
                                                {{ render(controller('UserBundle:Notification:notificationBar')) }}
                                            </div>
                                        </div>


                                        <div class="header-maincart">
                                            <div class="cart-container">
                                                <a href="{{ path('cart') }}" title="Mon Panier" class="icon-cart-header skip-link">
                                                    <span class="total"><strong class="fa fa-shopping-bag" aria-hidden="true"></strong><span class="hidden-mobile">{{ 'header.my-cart'|trans }}</span> <span class="numb main-numb">{{ app.session.get('cart')|length }}</span></span>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="top-nav-wrapper" class="top-nav-wrapper {% if 'DepositBundle' in app.request.attributes.get('_controller') or 'CartBundle' in app.request.attributes.get('_controller') %}non-sticky{% else %}sticky{% endif %} hidden-xs">
                <div class="container">
                    <div class="navigation-wrapper hidden-xs">
                        <div class="main-navigation">
                            <ul class="top-navigation">
                                <li class="level0 home level-top">
                                    <a href="{{ path('homepage') }}"
                                       class="level-top"><span>{{ 'header.menu-home'|trans }}</span></a>
                                </li>
                                {{ render(controller('AppBundle:Default:categories')) }}
                                <li class="level0 nav-end nav-right nav-sell level-top">
                                    <a href="{{ path('sell_category') }}" class="level-top">{{ 'header.menu-sell'|trans }}</a>
                                </li>
                                <li class="level0 nav-end nav-right nav-discover level-top">
                                    <a href="{{ path('about') }}" class="level-top">{{ 'header.menu-about'|trans }}</a>
                                </li>
                                <li class="level0 nav-end nav-right nav-discover level-top">
                                    <a href="{{ path('blog') }}" class="level-top">{{ 'header.menu-blog'|trans }}</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div><!-- #top-nav-wrapper -->
        </div>
    </div>
</header>


{% if app.user and (app.user.limitedBuyer or app.user.limitedSeller) %}

    <div class="alert alert-danger alert-dismissible alert-message-email text-center" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Fermer"><span aria-hidden="true">&times;</span></button>
        {{ 'header.limited-user-alert'|trans }}<br/>
