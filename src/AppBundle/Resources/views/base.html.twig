                                                                <li><a href="{{ path('user_account_wishlist') }}" class="link-wishlist">{{ 'header.account-menu-wishlist'|trans }}</a></li>
                                                                <li class=" last"><a class="log-in" href="{{ path('fos_user_security_logout') }}">{{ 'header.account-menu-logout'|trans }}</a></li>
                                                            {% else %}
                                                                <li class="first"><a href="{{ path('fos_user_security_login') }}" title="Connexion">{{ 'header.account-menu-logout'|trans }}</a></li>
                                                                <li class="last"><a href="{{ path('fos_user_registration_register') }}" title="Connexion">{{ 'header.account-menu-logout'|trans }}</a></li>
                                                            {% endif %}
