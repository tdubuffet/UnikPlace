<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>{% block title %}{{ (app.request.get('_route') ~ '.title')|trans({"%siteName%" :  siteName}, 'seo') }} {% endblock %}</title>

    {% set metaDescription =  (app.request.get('_route') ~ '.description')|trans({"%siteName%" :  siteName}, 'seo') %}

    {% if metaDescription == '' or  metaDescription == (app.request.get('_route') ~ '.description') %}
        {% set metaDescription = 'default.description'|trans({"%siteName%" :  siteName}, 'seo') %}
    {% endif %}

    <meta name="description" content="{% block description %}{{ metaDescription }}{% endblock %}">

    {% block meta %}{% endblock %}

    <!-- Bootstrap -->
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/master.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body {% block bodyclass %}{% endblock %}>
{% if app.environment == 'prod' %}
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function () {
                        (i[r].q = i[r].q || []).push(arguments)
                    }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-83010047-1', 'auto');
        ga('send', 'pageview');

    </script>
{% endif %}

<!-- // HEADER // -->
<header class="header-wrapper">
    <div class="header-container">
        <div class="main-header">
            <div class="container">
                <div class="header-content mobile-wrapper visible-xs">
                    <div id="mobile-sticky" class="mobile-content-inner {% if 'DepositBundle' in app.request.attributes.get('_controller') or 'CartBundle' in app.request.attributes.get('_controller') %}non-sticky{% else %}sticky{% endif %}">
                        <div class="header-top-left">
                            <div class="top-left-inner">
                                <div class="header-search">
                                    <div class="search-container">
                                        <div class="overwrite-setting skip-link" onclick="Common.mobileSkipLink(this);" data-target-element="#navbar-search">
                                            <i class="fa fa-search"></i>
                                        </div>
                                        <div id="navbar-search" class="switcher-content skip-content">
                                            <form id="search_mb_mini_form" action="{{ path('search') }}" method="get">
                                                <div class="input-group form-search">
                                                    <div class="input-group-btn category-filter">
                                                        <button type="button" class="btn dropdown-toggle" data-toggle="dropdown">
                                                            <span class="category-label">Tout </span>&nbsp;
                                                            <i class="fa fa-angle-down"></i>
                                                        </button>
                                                        {{ render(controller('AppBundle:Default:searchcategories')) }}
                                                    </div>
                                                    <input type="hidden" name="cat" class="search-category" value="{{ app.request.query.get('cat') }}"/>
                                                    <input id="mb_search"
                                                           type="search"
                                                           name="q"
                                                           value="{{ app.request.query.get('q') }}"
                                                           class="input-text required-entry"
                                                           maxlength="128"
                                                           placeholder="{{ 'header.search-placeholder'|trans }}"
                                                    />
                                                    <button type="submit" title="Search" class="search-button"><span><span><i class="fa fa-search"></i></span></span></button>
                                                </div>
                                                <div id="search_mb_autocomplete" class="search-autocomplete" style="display: none"></div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <!-- navigation BOF -->
                                <div class="navbar visible-xs">
                                    <div class="navbar">
                                        <div class="navbar-header skip-link" onclick="Common.mobileSkipLink(this);" data-target-element="#navbar-toggle">
                                            <button class="navbar-toggle collapsed" type="button" data-toggle="collapse">
                                                <span class="sr-only">Toggle navigation</span>
                                                <span class="icon-bar"></span>
                                                <span class="icon-bar"></span>
                                                <span class="icon-bar"></span>
                                            </button>
                                        </div>
                                        <nav id="navbar-toggle" class="bs-navbar-collapse skip-content">
                                            <ul class="nav-accordion nav-mobile-accordion">

                                                <li class="level0 home level-top">
                                                    <a href="{{ path('homepage') }}"
                                                       class="level-top"><span>{{ 'header.menu-home'|trans }}</span></a>
                                                </li>
                                                <li class="level0 home level-top">
                                                    <a href="{{ path('sell_category') }}"
                                                       class="level-top"><span>{{ 'header.menu-sell'|trans }}</span></a>
                                                </li>
                                                {{ render(controller('AppBundle:Default:mobilecategories')) }}
