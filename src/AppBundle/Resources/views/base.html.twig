                                         maxlength="128"
                                         placeholder="Rechercher sur le site..." />
                                  <button type="submit" title="Search" class="button"><span><span><i class="fa fa-search"></i></span></span></button>
                                </div>
                                <div id="search_autocomplete" class="search-autocomplete" style="display: none"></div>
                              </form>





                            </div>
                          </div>
                        </div>

                        <div class="header-account-action" id="notificationLi">
                          <div class="account-container">
                            {{ render(controller('UserBundle:Notification:notificationBar')) }}
                          </div>
                        </div>


                        <div class="header-account-action">
                          <div class="account-container">
                            <a href="{{ path('fos_message_inbox') }}" title="Ma messagerie" class="icon-header skip-link">
                              <span class="total"><strong class="fa fa fa-envelope" aria-hidden="true"></strong>{% if app.user and fos_message_nb_unread() > 0 %}<span class="count-numb">{{ fos_message_nb_unread() }}</span>{% endif %}</span>
                            </a>
                          </div>
                        </div>


                        <div class="header-maincart">
                          <div class="cart-container">
                            <a href="{{ path('cart') }}" title="Mon Panier" class="icon-cart-header skip-link">
                              <span class="total"><strong class="fa fa-shopping-bag" aria-hidden="true"></strong><span class="hidden-mobile">Mon Panier</span> <span class="numb main-numb">{{ app.session.get('cart')|length }}</span></span>
                            </a>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div id="top-nav-wrapper" class="top-nav-wrapper hidden-xs">
            <div class="container">
              <div class="navigation-wrapper hidden-xs">
                <div class="main-navigation">
                  <ul class="top-navigation">
                    <li class="level0 home level-top">
                      <a href="{{ path('homepage') }}"
                         class="level-top"><span>Accueil</span></a>
                    </li>
                    {{ render(controller('AppBundle:Default:categories')) }}
                    <li class="level0 nav-end level-top">
                      <a href="totheblog" class="level-top">Blog</a>
                      <a href="{{ path('sell_category') }}" class="level-top">Vendez</a>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </div><!-- #top-nav-wrapper -->
        </div>
      </div>
    </header>


